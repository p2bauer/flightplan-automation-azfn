{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Node Functions",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "preLaunchTask": "func: host start"
        }, 
        {
            "type": "node",
            "request": "launch",
            "name": "Jest Tests",
            "program": "${workspaceRoot}/src/node_modules/jest/bin/jest.js",
            "args": [
                "-i", 
                "${workspaceRoot}/src/QueueSearchFn/index.test.js", 
                "${workspaceRoot}/src/ProcessSearchFn/index.test.js"
            ],
            "cwd": "${workspaceRoot}/src", 
            "internalConsoleOptions": "openOnSessionStart"
          }
    ]
}